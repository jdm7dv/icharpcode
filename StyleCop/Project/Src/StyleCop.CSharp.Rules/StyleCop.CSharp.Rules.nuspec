<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>StyleCop.CSharp.Rules</id>
    <version>$version$</version>
    <authors>StyleCop contributors, Andy Reeves, Chris Dahlberg</authors>
    <description>Contains C# StyleCop analyzers that can be incorporated into other projects (such as ReSharper).</description>
    <releaseNotes></releaseNotes>
    <language>en-US</language>
    <projectUrl>https://github.com/StyleCop/StyleCop</projectUrl>
    <licenseUrl>https://github.com/StyleCop/StyleCop/blob/master/License.html</licenseUrl>
    <iconUrl>https://raw.githubusercontent.com/StyleCop/StyleCop/master/logo.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <file src="de-DE\StyleCop.CSharp.Rules.resources.dll" target="lib\net40\de-DE" />
    <file src="en-GB\StyleCop.CSharp.Rules.resources.dll" target="lib\net40\en-GB" />
    <file src="es-MX\StyleCop.CSharp.Rules.resources.dll" target="lib\net40\es-MX" />
    <file src="fr-FR\StyleCop.CSharp.Rules.resources.dll" target="lib\net40\fr-FR" />
    <file src="pl-PL\StyleCop.CSharp.Rules.resources.dll" target="lib\net40\pl-PL" />
    <file src="pt-BR\StyleCop.CSharp.Rules.resources.dll" target="lib\net40\pt-BR" />
    <file src="ru-RU\StyleCop.CSharp.Rules.resources.dll" target="lib\net40\ru-RU" />

    <!-- These files are from the StyleCop project but are included in this NuGet package. They are only needed
      when running analyzers. -->
    <file src="mssp7en.dll" target="build" />
    <file src="mssp7en.lex" target="build" />
    <file src="Settings.StyleCop" target="content" />
    <file src="StyleCop.CSharp.Rules.targets" target="build" />
  </files>
</package>